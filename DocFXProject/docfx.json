{
  "metadata": [
    {
      "src": [
        {
          "src" : "../Unity",
          "files": [
          	"JCMG.JTween.csproj"
          ]          
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "../Docs",
    "globalMetadataFiles": [],
    "globalMetadata": {
	    "_enableSearch": true,
	    "_appTitle": "JTween",
	    "_appFaviconPath": "images/app_icon.png",
	    "_appLogoPath": "images/app_icon_navbar.png",
	    "_appFooter": "Copyright Â© 2019 Jeff Campbell<br>Generated by <strong>DocFX</strong>",
	    "_gitContribute": {
	        "repo": "https://github.com/jeffcampbellmakesgames/unity-jtween.git",
	        "branch": "develop",
	    }
	},
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "template": [
            "default",
            "templates/darkfx"
    ]
  }
}